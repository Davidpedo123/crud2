@model crud2.OrdenCompra.Domain.Entidades.Proveedor
@{
    ViewData["Title"] = "Detalle Proveedor";
}
<h1>@ViewData["Title"]</h1>
<dl class="row">
    <dt class="col-sm-2">ID</dt><dd class="col-sm-10">@Model.ProveedorId</dd>
    <dt class="col-sm-2">Nombre</dt><dd class="col-sm-10">@Model.Nombre</dd>
    <dt class="col-sm-2">Contacto</dt><dd class="col-sm-10">@Model.Contacto</dd>
    <dt class="col-sm-2">Teléfono</dt><dd class="col-sm-10">@Model.Telefono</dd>
    <dt class="col-sm-2">Email</dt><dd class="col-sm-10">@Model.Email</dd>
    <dt class="col-sm-2">Dirección</dt><dd class="col-sm-10">@Model.Direccion</dd>
</dl>
<a asp-action="Index" class="btn btn-secondary">Volver</a>
